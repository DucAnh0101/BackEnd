{
  "openapi": "3.0.1",
  "info": {
    "title": "CustomStore",
    "version": "1.0"
  },
  "paths": {
    "/api/F_Q/questions": {
      "get": {
        "tags": [
          "F_Q"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/QuestionRes"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/QuestionRes"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/QuestionRes"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/F_Q/questions/{id}": {
      "get": {
        "tags": [
          "F_Q"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              }
            }
          }
        }
      }
    },
    "/api/F_Q/questions/group/{Group_id}": {
      "get": {
        "tags": [
          "F_Q"
        ],
        "parameters": [
          {
            "name": "Group_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionRes"
                }
              }
            }
          }
        }
      }
    },
    "/api/JWT/login": {
      "post": {
        "tags": [
          "JWT"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/JWT/reset-password": {
      "post": {
        "tags": [
          "JWT"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ResetPassReq"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ResetPassReq"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ResetPassReq"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginRequest": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "QuestionRes": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "groupName": {
            "type": "string",
            "nullable": true
          },
          "questionText": {
            "type": "string",
            "nullable": true
          },
          "answerText": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ResetPassReq": {
        "required": [
          "citizenId",
          "dob",
          "email",
          "phoneNumber",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "citizenId": {
            "minLength": 1,
            "type": "string"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}